{
  "name": "webpack-project",
  "version": "1.0.0",
  "description": "Template for frontend develop base on Webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --mode development --config webpack.dev.js",
    "build": "webpack --mode production --config webpack.prod.js",
    "watch": "webpack --watch",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sergeykonovalenko/webpack-project.git"
  },
  "author": "Sergey Konovalenko",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "autoprefixer": "^10.4.23",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.4",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.5",
    "mini-css-extract-plugin": "^2.9.4",
    "path": "^0.12.7",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "sass": "^1.97.1",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "svg-sprite-loader": "^6.0.11",
    "svgo": "^4.0.0",
    "svgo-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.16",
    "webpack": "^5.104.1",
    "webpack-bundle-analyzer": "^5.1.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-middleware": "^7.4.5",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@fancyapps/ui": "^6.1.7",
    "focus-visible": "^5.2.1",
    "jquery": "^3.7.1",
    "normalize.css": "^8.0.1"
  }
